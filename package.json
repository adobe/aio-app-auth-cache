{
  "name": "aio-app-auth-cache",
  "version": "1.0.1",
  "description": "An Openwhisk action exposing a cache interface",
  "main": "./src/action/cache.js",
  "scripts": {
    "start": "babel-watch ./test/server/index.js",
    "posttest": "eslint src test",
    "test": "npm run unit-tests",
    "unit-tests": "jest --ci",
    "prepublishOnly": "webpack",
    "whisk_wrapper": "echo \"var main = require('main-action').default;\" >> ${npm_package_name}-${npm_package_version}.js"
  },
  "author": {
    "name": "Himavanth",
    "email": "himar@adobe.com"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@adobe/aio-lib-core-logging": "^2.0.0",
    "@adobe/aio-lib-state": "^1.0.2",
    "@babel/runtime": "^7.22.6"
  },
  "devDependencies": {
    "@adobe/eslint-config-aio-lib-config": "^1.4.0",
    "@babel/cli": "^7",
    "@babel/core": "^7",
    "@babel/plugin-transform-destructuring": "^7.22.5",
    "@babel/plugin-transform-runtime": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "aes256": "^1.0.4",
    "babel-loader": "^8.0.6",
    "body-parser": "^1.15.2",
    "browserify": "^17",
    "chai": "^4.3.8",
    "chai-as-promised": "^6.0.0",
    "cls-bluebird": "^2.0.1",
    "compression": "^1.6.2",
    "cors": "^2.7.1",
    "encoding": "^0.1.12",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsdoc": "^37.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "express": "^4.14.0",
    "http-status": "^1.6.2",
    "isparta": "^4.0.0",
    "istanbul": "^0.3.17",
    "jest": "^29",
    "jest-junit": "^16",
    "mocha": "^10.2.0",
    "nock": "^9.0.2",
    "openwhisk": "^3.21.2",
    "replace": "^1.2.2",
    "request": "^2.79.0",
    "resource-router-middleware": "^0.7.0",
    "supertest": "^1.1.0",
    "supertest-as-promised": "^2.0.2",
    "typescript": "^4.5.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "browserify": {
    "transform": [
      [
        "babelify"
      ]
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "@babel/plugin-transform-runtime",
        {
          "helpers": false,
          "regenerator": true
        }
      ]
    ]
  },
  "keywords": [
    "serverless, openwhisk, authentication, passportjs"
  ],
  "readmeFilename": "README.md",
  "jest": {
    "collectCoverage": true,
    "testPathIgnorePatterns": [
      "<rootDir>/tests/fixtures/"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/tests/fixtures/"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ]
  }
}
